<table cellspacing="0" cellpadding="0" align="center"
    style="border-collapse:collapse;border-spacing:0px;table-layout:fixed!important;width:100%">
    <tbody>
        <tr style="border-collapse:collapse">
            <td align="center" style="padding:0;Margin:0">
                <table width="600" cellspacing="0" cellpadding="0" bgcolor="#ffffff" align="center"
                    style="border-collapse:collapse;border-spacing:0px;background-color:#ffffff">
                    <tbody>
                        <tr style="border-collapse:collapse">
                            <td align="left" style="padding:0;Margin:0">
                                <table width="100%" cellspacing="0" cellpadding="0"
                                    style="border-collapse:collapse;border-spacing:0px">
                                    <tbody>
                                        <tr style="border-collapse:collapse">
                                            <td width="600" valign="top" align="center" style="padding:0;Margin:0">
                                                <table width="100%" cellspacing="0" cellpadding="0"
                                                    style="border-collapse:collapse;border-spacing:0px">
                                                    <tbody>
                                                        <tr style="border-collapse:collapse">
                                                            <td style="padding:0;Margin:0" align="center">
                                                                <img src="<%= logoUrl %>" width="100%"
                                                                    style="display:block;border:0;outline:none;text-decoration:none">
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td style="color: #000; padding: 10px 0px;">
                                                                Hi
                                                                <b>
                                                                    <%= name %>,
                                                                </b>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td style="font-size: 14px; padding: 0px 0 10px 0px;">
                                                                Thank You for your registration, we will get back to you
                                                                shortly .
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td style="color: #000; padding: 10px 0px;">
                                                                <b>Your Username is </b> <%= username %>
                                                            </td>
                                                        </tr>
                                                        <!-- <tr>
                                                            <td style="color: #000; padding: 10px 0px;">
                                                                <b>Password </b>: <%= password %>
                                                            </td>
                                                        </tr> -->
                                                        <tr>
                                                            <td style="font-size: 14px; padding: 0px 0 10px 0px;">
                                                                Please click on the button below to login.
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td style="text-align: left">
                                                                <a href="<%= link %>"
                                                                    style="color: #ffffff; text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25); background-color: #2aafea; border:1px solid transparent; border-color: #2aafea #2aafea #1494cd; display: inline-block; padding: 8px 18px; font-weight: 600; font-size: 14px; text-decoration: none; margin: 0 0 0px 0px;"
                                                                    target="_blank">Login</a>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td style="padding: 15px 0 5px 0px;">Thanks</td>
                                                        </tr>
                                                        <tr>
                                                            <td style="padding: 5px 0px 20px 0px;">Barbeque Nation Hospitality Ltd</td>
                                                        </tr>
                                                        <tr>
                                                            <td
                                                                style="padding: 5px 0 10px 0px;border-top: 1px solid #dadada;">
                                                                Note: For your privacy and protection, please do not
                                                                forward this
                                                                mail to anyone</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </td>
        </tr>
    </tbody>
</table>
